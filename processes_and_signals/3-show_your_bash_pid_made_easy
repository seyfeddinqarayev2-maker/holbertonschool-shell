#!/usr/bin/env bash
# Bu skript adında 'bash' olan proseslərin PID və adlarını göstərir
# ps istifadə olunmur

for pid in /proc/[0-9]*; do
    if [ -r "$pid/comm" ]; then
        name=$(cat "$pid/comm")
        if echo "$name" | grep -q "bash"; then
            echo "$(basename "$pid") $name"
        fi
    fi
done

